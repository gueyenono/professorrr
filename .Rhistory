add_to_random <- function(x, n, addition){
changed_i <- sample(length(x), size = n, replace = FALSE)
x[changed_i] <- x[changed_i] + addition
x
}
substract_from_random <- function(x, n, substraction){
changed_i <- sample(length(x), size = n, replace = FALSE)
x[changed_i] <- x[changed_i] - substraction
x
}
